						"5": { "name": "markup.bold.string.tmlanguage", "patterns": [ { "include": "source.syntax.json.tmLanguage#escape-characters" } ] }
					}
				}
			]
		},
		"subroutine": {
			"comment": "\\g<99> \\g'name'",
			"match": "(\\\\{2}g)(?>(<)([^>\"]*)(>)?|(')([^'\"]*)(')?)",
			"captures": {
				"1": { "name": "string.regexp.tmlanguage" },
				"2": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
				"3": { "patterns": [ {"include": "#capture-group-calling-name"} ] },
				"4": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
				"5": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
				"6": { "patterns": [ {"include": "#capture-group-calling-name"} ] },
				"7": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" }
			}
		},
		"backreference": {
			"patterns": [
				{
					"comment": "\\1",
					"match": "(\\\\{2})([1-9]\\d{,2})(\\d*)",
					"captures": {
						"1": { "name": "string.regexp.tmlanguage"	 },
						"2": { "name": "constant.numeric.tmlanguage" },
						"3": { "name": "invalid.tmlanguage"			 }
					}
				},
				{
					"comment": "\\k<99> \\k'name'",
					"match": "(\\\\{2}k)(?>(<)([^>\"]*)(>)?|(')([^'\"]*)(')?)",
					"captures": {
						"1": { "name": "string.regexp.tmlanguage" },
						"2": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
						"3": { "patterns": [ {"include": "#capture-group-calling-name"} ] },
						"4": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
						"5": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" },
						"6": { "patterns": [ {"include": "#capture-group-calling-name"} ] },
						"7": { "name": "meta.embedded.tmlanguage support.function.tmlanguage" }
					}
				}
			]
		},
		"quantifier": {
			"comment": "+ * ? {1,9}",
			"patterns": [
				{
					"match": "\\G[+*?]",
					"name": "invalid.tmlanguage markup.underline.tmlanguage"
				},
				{
					"match": "\\G{(?>\\d+,?\\d*|,\\d+)}",
					"name": "invalid.tmlanguage markup.underline.tmlanguage"
				},
				{
					"comment": "As soon as theres a {0} quantifier, all other quantifiers are nullified",
					"match": "(\\g<4>)?({0*,?0+})((?(<1>)\\g<4>?|((?>[+*?]++|{(?>\\d+,?\\d*|,\\d+)})+)))",
					"captures": {
						"1": { "name": "meta.embedded.tmlanguage punctuation.definition.tag.tmlanguage" },
						"2": { "patterns": [ {"include": "#quantifier"} ] },
						"3": { "name": "meta.embedded.tmlanguage punctuation.definition.tag.tmlanguage" }
					}
				},
				{
					"comment": "+ * ? ++ +? *+ *? ?+ ?? \n  ? creates a lazy quantifier and + a possessive quantifier",
					"match": "[+*?]([+?])?",
					"captures": {
						"0": { "name": "keyword.tmlanguage" },
						"1": { "name": "strong.tmlanguage" }
					}
				},
				{
					"match": "({)(?<number>(0*)\\d+)?(,)?((?(<number>)(?>(0*)\\d+)?|(0*)\\d+))(})(\\?)?",
					"captures": {
						"1": { "name": "meta.embedded.tmlanguage keyword.tmlanguage" },
						"2": { "name": "constant.numeric.tmlanguage" },
						"3": { "name": "punctuation.definition.tag.tmlanguage" },
						"4": { "name": "keyword.control.tmlanguage" },
						"5": { "name": "constant.numeric.tmlanguage" },
						"6": { "name": "punctuation.definition.tag.tmlanguage" },
						"7": { "name": "punctuation.definition.tag.tmlanguage" },
						"8": { "name": "meta.embedded.tmlanguage keyword.tmlanguage" },
						"9": { "name": "keyword.tmlanguage strong.tmlanguage" }
					}
				}
			]
		},
		"anchor": {
			"comment": "$ ^ \\A \\B \\b \\G \\K \\Z \\z",
			"match": "[$^]|\\\\{2}[ABbGKZz]",
			"name": "variable.tmlanguage"
		},
		"alternation": {
			"comment": "|",
			"match": "\\|",
			"name": "keyword.control.tmlanguage"
		},
		"regex": {
			"comment": "Match anything left over and all non-special charaters",
			"match": ".[^\\x0-\\x20\"-$(-+.?\\[\\\\^{|\\x7F]*",
			"name": "entity.name.label.tmlanguage strong.string.tmlanguage"
		},
		"invalid": {
			"comment": "invalid single escaped characters including newlines, rouge right brackets, unescaped double quotes and any invalid control characters",
			"match": "\\\\.?|\\)|\"|[\\x0-\\x1F]|\\x7F",
			"name": "invalid.string.tmlanguage markup.underline.tmlanguage"
		},
		"test": {
			"match": "(?x){2,3}{2,3}({2,3}(?:{2,3})){2,3}{,3}{2,}{2} {000} *{0} {0}* *{0}* {0}{0}{0} {0}{2,3} {2,3}{0}{2,3} {2,3\\} () (?<= (?= (?>) )? ) (?= (?= )? ) [a-z&&]u°𐐷Ꚛ�𐀀𐃘[�𐃘] \\g<99> \\x555 QabcE $ \\$ \\1234567890 abcde\fghijklm\nopq\rs\t\u0000vwxyz bfnrtu \\a\\b\\cZ\\d\\e\\f\\g<0>\\h\\i\\j\\k'0'\\l\\m\\n\\o\\p{L}\\q\\r\\s\\t\\u0000\\v\\w\\x{13FFFF}\\y\\z °Ççë£ ||| ABCDEFGHIJKLMNOPQRSTUVWXYZ`1234567890-=\\[]\\\\;',./ ~!@\\#$%^&*()_+{}|:\"<>?   \\A\\B\\C-]\\D\\E\\F\\G\\H\\I\\J\\K\\L\\M-a\\N\\O\\P{L}\\Q\\R\\S\\T\\U\\V\\W\\X\\Y\\Z \\x{201E} \\p{L} \\p{-L etterLaaLaa--LL--LL--} \\`\\1\\2\\3\\4\\5\\6\\7\\8\\9\\0\\-\\=\\[\\]\\\\\\;\\'\\,\\.\\/\\ \\~\\!\\@\\#\\$\\%\\^\\&\\*\\(\\)\\_\\+\\{\\}\\|\\:\\\"\\<\\>\\? [[:^upper:]] At aT aa 1000 Q.E q.e a$abc#comment\n (?-x) #notcomment\n/z"
		}
