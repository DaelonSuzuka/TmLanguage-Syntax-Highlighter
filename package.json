	"contributes": {
		"languages": [
			{
				"id": "json-tmlanguage",
				"aliases": [
					"RedCMD.JSON.TextMateLanguage"
				],
				"extensions": [
					".tmLanguage.json",
					".JSON-tmLanguage",
					".tmGrammar.json"
				],
				"filenames": [
					"tmLanguage.json"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "regex-tmlanguage",
				"aliases": [
					"RedCMD.Regex.TextMateLanguage"
				],
				"extensions": [
					".regex.json",
					".tmRegexLanguage.json",
					".JSON-regex-tmLanguage"
				],
				"filenames": [
					"regex.json"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "json-tmlanguage",
				"scopeName": "source.syntax.json.tmlanguage",
				"path": "./syntaxes/json.tmLanguage.json"
			},
			{
				"language": "regex-tmlanguage",
				"scopeName": "source.syntax.regex.tmlanguage",
				"path": "./syntaxes/regex.tmLanguage.json"
			}
		]
	},
